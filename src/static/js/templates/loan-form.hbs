<form id="lc-input-{{form_id}}" class="lc-input comparison-input-column-{{form_id}} no-labels wrapper">
  <a href="#lc-input-{{form_id}}" class="lc-save-link lc-toggle first-save"><span class="cf-icon cf-icon-save"></span> Save inputs</a>
  <h3 class="visuallyhidden">Loan {{form_id}}</h3>
  <div class="input-subsection">
    <label for="location-{{form_id}}" class="label-float">State</label>
    <div class="input-wrap"> <!-- use with columns with floated labels -->
      <div class="select-content">
        <select name="location" class="recalc" id="location-{{form_id}}">
          <option value="AL">Alabama</option>
          <option value="AK">Alaska</option>
          <option value="AZ">Arizona</option>
          <option value="AK">Arkansas</option>
          <option value="CA">California</option>
          <option value="CO">Colorado</option>
          <option value="CT">Connecticut</option>
          <option value="DE">Delaware</option>
          <option value="DC">District of Columbia</option>
          <option value="FL">Florida</option>
          <option value="GA">Georgia</option>
          <option value="HI">Hawaii</option>
          <option value="ID">Idaho</option>
          <option value="IL">Illinois</option>
          <option value="IN">Indiana</option>
          <option value="IA">Iowa</option>
          <option value="KS">Kansas</option>
          <option value="KY">Kentucky</option>
          <option value="LA">Louisiana</option>
          <option value="ME">Maine</option>
          <option value="MD">Maryland</option>
          <option value="MA">Massachusetts</option>
          <option value="MI">Michigan</option>
          <option value="MN">Minnesota</option>
          <option value="MS">Mississippi</option>
          <option value="MO">Missouri</option>
          <option value="MT">Montana</option>
          <option value="NE">Nebraska</option>
          <option value="NV">Nevada</option>
          <option value="NH">New Hampshire</option>
          <option value="NJ">New Jersey</option>
          <option value="NM">New Mexico</option>
          <option value="NY">New York</option>
          <option value="NC">North Carolina</option>
          <option value="ND">North Dakota</option>
          <option value="OH">Ohio</option>
          <option value="OK">Oklahoma</option>
          <option value="OR">Oregon</option>
          <option value="PA">Pennsylvania</option>
          <option value="RI">Rhode Island</option>
          <option value="SC">South Carolina</option>
          <option value="SD">South Dakota</option>
          <option value="TN">Tennessee</option>
          <option value="TX">Texas</option>
          <option value="UT">Utah</option>
          <option value="VT">Vermont</option>
          <option value="VA">Virginia</option>
          <option value="WA">Washington</option>
          <option value="WV">West Virginia</option>
          <option value="WI">Wisconsin</option>
          <option value="WY">Wyoming</option>
        </select>
      </div> <!-- /.select-content -->
    </div> <!-- /.input-wrap -->

    <label for="credit-score-select-{{form_id}}" class="label-float ">Credit Score</label>
    <div class="input-wrap"> <!-- use with columns with floated labels -->
      <div class="select-content">
        <select name="credit-score-select" class="recalc" id="credit-score-select-{{form_id}}">
          <option value="600">600 - 620</option>
          <option value="620">621 - 640</option>
          <option value="640">641 - 660</option>
          <option value="660">661 - 680</option>
          <option value="680">681 - 700</option>
          <option value="700" selected="selected">701 - 720</option>
          <option value="720">721 - 740</option>
          <option value="740">741 - 760</option>
          <option value="760">761 - 780</option>
          <option value="780">781 - 800</option>
          <option value="800">801 - 820</option>
          <option value="820">821 - 840</option>
          <option value="840">841+</option>
        </select>
      </div> <!-- /.select-content -->
    </div> <!-- /.input-wrap -->
  </div> <!-- /.input-subsection -->

  <div class="input-subsection pricing">
    <div class="calc-content">
      <h5 class="lc-subhead label-float">Loan amount</h5>
      <div class="input-wrap"> <!-- use with columns with floated labels -->
        <span class="loan-amount-display-{{form_id}}">$180,000</span>
      </div> <!-- /.input-wrap -->
    </div> <!-- ./calc-content-->

    <div class="input-content">
      <label for="house-price-input-{{form_id}}" class="label-float ">House price</label>
      <div class="input-wrap"> <!-- use with columns with floated labels -->
        <input type="text" placeholder="$200,000" name="house-price-input" id="house-price-input-{{form_id}}" class="recalc">
      </div> <!-- /.input-wrap -->
    </div> <!-- ./input-content-->

    <div class="input-content">
      <label for="percent-down-input-{{form_id}}" class="label-float ">Down payment <span class="visuallyhidden">Percentage</span></label>
      <div class="input-wrap"> <!-- use with columns with floated labels -->
        <div class="percent-input small-input">
          <span class="unit">%</span>
          <input type="text" placeholder="10" maxlength="2" name="percent-dp-input" id="percent-dp-input-{{form_id}}" class="recalc">
        </div>
        <label for="down-payment-input-{{form_id}}" class="visuallyhidden">Down payment amount</label>
        <div class="mid-input">
          <input type="text" placeholder="$20,000" name="down-payment-input" id="down-payment-input-{{form_id}}" class="recalc">
        </div>
      </div><!-- /.input-wrap -->
    </div> <!-- ./input-content-->
  </div> <!-- /.input-subsection -->

  <div class="input-subsection">
    <div class="calc-content">
      <h5 class="lc-subhead label-float">Loan option</h5>
      <div class="input-wrap"> <!-- use with columns with floated labels -->
        <span id="lc-summary-year-{{form_id}}">30</span>-year <span id="lc-summary-structure-{{form_id}}">fixed</span> <span id="lc-summary-type-{{form_id}}">conventional</span>
      </div> <!-- /.input-wrap -->
    </div> <!-- ./calc-content-->

    <label for="rate-structure-select-{{form_id}}" class="label-float ">Rate structure</label>
    <div class="input-wrap"> <!-- use with columns with floated labels -->
      <div class="select-content">
        <select name="rate-structure-select" class="recalc" id="rate-structure-select-{{form_id}}">
          <option value="fixed">Fixed</option>
          <option value="ARM">Adjustable</option>
        </select>
      </div> <!-- /.select-content -->
    </div><!-- /.input-wrap -->

    <label for="loan-term-select-{{form_id}}" class="label-float ">Loan term</label>
    <div class="input-wrap"> <!-- use with columns with floated labels -->
      <div class="select-content">
        <select name="loan-term-select" class="recalc" id="loan-term-select-{{form_id}}">
          <option value="30">30 years</option>
          <option value="15">15 years</option>
        </select>
      </div> <!-- /.select-content -->
    </div><!-- /.input-wrap -->

    <label for="loan-type-select-{{form_id}}" class="label-float ">Loan type</label>
    <div class="input-wrap"> <!-- use with columns with floated labels -->
      <div class="select-content">
        <select name="loan-type-select" class="recalc" id="loan-type-select-{{form_id}}">
          <option value="conf">Conventional</option>
          <option value="fha">FHA</option>
          <option value="va">VA</option>
        </select>
      </div> <!-- /.select-content -->
    </div><!-- /.input-wrap -->
  </div> <!-- /.input-subsection -->

  <div class="input-subsection">
    <label for="arm-type-select-{{form_id}}" class="label-float ">ARM type</label>
    <div class="input-wrap"> <!-- use with columns with floated labels -->
      <div class="select-content">
        <select name="arm-type-select" class="recalc" id="arm-type-select-{{form_id}}">
          <option value="3/1">3/1</option>
          <option value="5/1">5/1</option>
          <option value="7/1">7/1</option>
          <option value="10/1">10/1</option>
        </select>
      </div> <!-- /.select-content -->
    </div><!-- /.input-wrap -->
  </div> <!-- /.input-subsection -->

  <div class="input-subsection">
    <fieldset id="points-{{form_id}}" class="radio-fieldset input-content">
      <label class="label-float ">Discount point <br/>and credits</label>
      <div class="input-wrap"> <!-- use with columns with floated labels -->
        <div class="inline-radio lc-radio">
          <input type="radio" name="discount" id="discount-negative-two-{{form_id}}" value="-2" />
          <label for="discount-negative-two">-2</label>
        </div>
        <div class="inline-radio lc-radio">
          <input type="radio" name="discount" id="discount-negative-one-{{form_id}}" value="-1" />
          <label for="discount-negative-one">-1</label>
        </div>
        <div class="inline-radio lc-radio">
          <input type="radio" name="discount" id="discount-zero-{{form_id}}" value="0" checked="checked" />
          <label for="discount-zero">0</label>
        </div>
        <div class="inline-radio lc-radio">
          <input type="radio" name="discount" id="discount-one-{{form_id}}" value="1" />
          <label for="discount-one">1</label>
        </div>
        <div class="inline-radio lc-radio">
          <input type="radio" name="discount" id="discount-two-{{form_id}}" value="2" />
          <label for="discount-two">2</label>
        </div>
        <div class="inline-radio lc-radio">
          <input type="radio" name="discount" id="discount-three-{{form_id}}" value="3" />
          <label for="discount-three">3</label>
        </div>
      </div> <!-- /.input-wrap -->
    </fieldset>
  </div> <!-- /.input-subsection -->

  <div class="input-subsection">
    <label for="interest-rate-select-{{form_id}}" class="label-float">Interest rate</label>
    <div class="input-wrap"> <!-- use with columns with floated labels -->
      <div class="select-content">
        <select name="interest-rate-select" class="recalc" id="interest-rate-select-{{form_id}}">
          <option value="3.25">3.25%</option>
          <option value="3.75">3.75%</option>
          <option value="4">4%</option>
          <option value="4.5">4.5%</option>
          <option value="4.75">4.75%</option>
        </select>
      </div> <!-- /.select-content -->
    </div><!-- /.input-wrap -->
  </div> <!-- /.input-subsection -->
  <a href="#lc-input-{{form_id}}" class="lc-save-link lc-toggle second-save"><span class="cf-icon cf-icon-save"></span> Save inputs</a>
</form> <!-- /.comparison-input-column -->